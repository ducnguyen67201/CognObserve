{
  "name": "cognobserve",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "AI Platform Monitoring & Observability",
  "scripts": {
    "dev": "doppler run -- turbo dev",
    "dev:no-doppler": "turbo dev",
    "build": "doppler run -- turbo build",
    "lint": "turbo lint",
    "test": "doppler run -- turbo test",
    "clean": "turbo clean && rm -rf node_modules",
    "db:generate": "doppler run -- turbo db:generate",
    "db:push": "doppler run -- turbo db:push",
    "db:migrate": "doppler run -- turbo db:migrate",
    "db:seed": "doppler run -- pnpm --filter @cognobserve/db db:seed",
    "db:studio": "doppler run -- pnpm --filter @cognobserve/db db:studio",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "devDependencies": {
    "prettier": "^3.4.2",
    "turbo": "^2.5.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=24.0.0"
  }
}
